{
  "name": "tokenmetrics-mobile-app",
  "version": "2.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=test expo start -c",
    "start:build": "NODE_ENV=test expo start --no-dev --minify",
    "android": "NODE_ENV=test expo run:android",
    "ios": "NODE_ENV=test expo run:ios",
    "web": "NODE_ENV=test expo start --web",
    "build:android:dev": "NODE_ENV=development eas build --profile development --platform android",
    "build:ios:dev": "NODE_ENV=development eas build --profile development --platform ios",
    "build:ios:preview": "NODE_ENV=test eas build --profile preview --platform ios",
    "submit:ios:preview": "NODE_ENV=test eas submit -p ios --profile preview",
    "build:ios:prod": "NODE_ENV=production eas build --platform ios",
    "submit:ios:prod": "NODE_ENV=production eas submit -p ios",
    "release:assemble": "NODE_ENV=test cd android && ./gradlew assembleRelease",
    "release:bundle": "NODE_ENV=test cd android && ./gradlew bundleRelease",
    "expo:build:ios": "NODE_ENV=test expo run:ios --configuration Release",
    "expo:build:android": "NODE_ENV=test expo run:android --variant release",
    "expo:prebuild": "expo prebuild --clean",
    "check-types": "tsc --noEmit --pretty",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky",
    "storybook-generate": "sb-rn-get-stories",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "test:update-snapshots": "jest -u --coverage=false",
    "lingui:extract": "lingui extract src/components src/navigation src/screens src/hooks --clean",
    "lingui:compile": "lingui compile --typescript"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.8.0",
    "@formatjs/intl-locale": "^4.2.10",
    "@formatjs/intl-pluralrules": "^5.4.3",
    "@hookform/resolvers": "^4.1.3",
    "@lingui/core": "^5.2.0",
    "@lingui/react": "^5.2.0",
    "@privy-io/expo": "^0.49.0",
    "@privy-io/expo-native-extensions": "^0.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/drawer": "^7.1.2",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "@reown/appkit-wagmi-react-native": "^1.2.2",
    "@sentry/react-native": "~6.3.0",
    "@shopify/flash-list": "1.7.3",
    "@shopify/react-native-skia": "1.5.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.67.1",
    "@walletconnect/react-native-compat": "^2.19.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "expo": "~52.0.37",
    "expo-apple-authentication": "~7.1.3",
    "expo-application": "~6.0.2",
    "expo-auth-session": "~6.0.3",
    "expo-av": "^15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.7",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.12",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "fast-text-encoding": "^1.0.6",
    "nativewind": "^4.1.23",
    "posthog-react-native": "^3.11.2",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-recaptcha-that-works": "^2.0.0",
    "react-native-root-toast": "^3.6.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "13.12.5",
    "uuid": "^11.1.0",
    "victory-native": "^41.16.1",
    "viem": "^2.23.6",
    "wagmi": "^2.14.12",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-get-random-values"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@lingui/babel-plugin-lingui-macro": "^5.2.0",
    "@lingui/cli": "^5.2.0",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-ondevice-actions": "^8.5.5",
    "@storybook/addon-ondevice-controls": "^8.5.5",
    "@storybook/react": "^8.6.3",
    "@storybook/react-native": "^8.5.5",
    "@testing-library/react-native": "^13.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-native-modals": "^0.22.4",
    "@types/react-native-typewriter": "^0.7.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@walletconnect/jsonrpc-types": "^1.0.4",
    "babel-loader": "^8.4.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-lingui": "^0.10.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.5",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "3.4.17",
    "typescript": "~5.8.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  }
}
